{
    "name": "Mozi",
    "description": "Converts speech to text and use the text converted to search through an online search engine.",
    "version": "1.0.0",
    "manifest_version": 2,
    "icons": {
      "256": "firefox-icon-256.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["mozi.js"]
      }
    ],
    "background": {
      "scripts": ["background.js"]
    }    
  }
  